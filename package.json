    "@types/node": "24.6.2",
    "eslint": "9.37.0",
    "eslint-plugin-playwright": "2.2.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.3",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0"
